&dsi0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	ports {
		dsi0_out: port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
			dsi0_out_panel: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&mipi_panel_in_dsi0>;
			};
		};
	};
	
	
	
	sq101d_panel: panel@0 {
		compatible = "SQ101D_Q5DI404_84H501";
		status = "okay";
		reg = <0>;
		power0-supply = <&reg_cldo4>;
		// enable-gpios = <&pio PH 18 GPIO_ACTIVE_HIGH>;
		// reset-gpios = <&pio PD 22 GPIO_ACTIVE_HIGH>;
		// backlight = <&backlight>;
		lcd_if = <4>;
		lcd_width = <136>;
		lcd_height = <217>;
		lcd_dclk_freq = <157>;
		lcd_frm = <0>;
		lcd_gamma_en = <0>;
		lcd_bright_curve_en = <0>;
		lcd_cmap_en = <0>;
		lcd_start_delay = <5>;
		lcd_dsi_if = <0>;
		lcd_dsi_lane = <4>;
		lcd_dsi_format = <0>;
		lcd_dsi_te = <0>;
		lcd_dsi_eotp = <0>;
		// pinctrl-names = "active", "sleep";
		pinctrl-names = "active";
		pinctrl-0 = <&dsi0_4lane_pins_a>;
		// pinctrl-1 = <&dsi0_4lane_pins_b>;
		panel-timing {
			clock-frequency = <156408000>; /* pixel clock */
			hback-porch = <50>;
			hactive = <1200>;
			hfront-porch = <70>;
			hsync-len = <10>;
			vback-porch = <20>;
			vactive = <1920>;
			vfront-porch = <16>;
			vsync-len = <4>;
			// unused now
			/*
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <1>;
			pixelclk-active = <1>;
			*/
		};
		ports {
			#address-cells = <1>;
			#size-cells = <0>;
			mipi_panel_in: port@0 {
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0>;
				mipi_panel_in_dsi0: endpoint@0 {
					reg = <0>;
					remote-endpoint = <&dsi0_out_panel>;
				};
			};
		};
	};
};